# Copy this file to .env and adjust values for your local environment.
# docker compose will automatically read .env in this directory.

# --- Core processing ---
MODEL=htdemucs
DEMUCS_DEVICE=cpu
DEMUCS_JOBS=1
# MP3 encoding: v0 (VBR V0) or cbr320 (CBR 320kbps)
MP3_ENCODING=cbr320

# --- Chunking & mixing ---
CHUNKING_ENABLED=true
CHUNK_MAX=16
CHUNK_OVERLAP_SEC=0.5
CROSSFADE_MS=200

# --- Corrupt input handling ---
# archive -> moves to ARCHIVE_DIR/rejects; quarantine -> moves to QUARANTINE_DIR
CORRUPT_DEST=archive

# --- MinIO / S3 mirroring (optional) ---
# These are used by the minio and minio-mirror services
MINIO_ACCESS_KEY=minioadmin
MINIO_SECRET_KEY=minioadmin
MINIO_BUCKET=instrumentals
MINIO_ENDPOINT=http://minio:9000
MINIO_USE_SSL=false
MINIO_REGION=us-east-1

# --- CPU limits (optional) ---
CPU_MAX_THREADS=0
CPU_AFFINITY=
CPU_NICE=0
FFMPEG_THREADS=0

# --- Docker Hub username (optional) ---
# Used by docker-compose.prebuilt.yml image reference substitution
DOCKERHUB_USERNAME=youruser
